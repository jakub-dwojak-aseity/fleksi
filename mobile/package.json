{
  "name": "fleksi-mobile",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios", 
    "web": "expo start --web",
    "gen:client": "openapi-generator-cli generate -i ../openapi/fleksi.yml -g typescript-axios -o src/api --additional-properties=supportsES6=true,withSeparateModelsAndApi=true,modelPackage=models,apiPackage=apis",
    "type-check": "tsc --noEmit",
    "lint": "echo 'Lint not configured yet'",
    "test": "echo 'Tests not configured yet'"
  },
  "dependencies": {
    "expo": "~51.0.28",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-screens": "3.31.1",
    "react-native-safe-area-context": "4.10.5",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@tanstack/react-query": "^5.56.2",
    "axios": "^1.7.7",
    "@react-native-async-storage/async-storage": "1.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@types/react-native": "~0.73.0",
    "typescript": "~5.3.3",
    "@openapitools/openapi-generator-cli": "^2.14.0"
  },
  "private": true
}